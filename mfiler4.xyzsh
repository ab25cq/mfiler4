    cursor -d 1 | var -local cursor_1dir

    mask -d 0 $(mask -d 1 |chomp)
    mask -dotdir -d 0 $(mask -d 1 -dotdir |chomp)
    mcd -d 0 $(path -d 1 | chomp)
    cursor_move -d 0 /$(cursor_1dir|chomp)
)

